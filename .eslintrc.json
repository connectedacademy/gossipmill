{
    "env": {
        "es6": true,
        "node": true
    },
    "globals": {
        "sails":true,
        "_":true,
        "Message":true,
        "SubscriptionManager":true,
        "Twitter":true,
        "User":true
    },
    "parserOptions": {
        "ecmaVersion": 8,
        "ecmaFeatures": {
            "jsx": false
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn"
    }
}